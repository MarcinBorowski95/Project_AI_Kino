<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.cinema.MapperJava.MovieMapper">

    <select id="getShowtimes" resultMap="showtimes">
        SELECT id_showtime FROM showtimes
    </select>

    <resultMap id="showtimes" type="com.cinema.Domain.User">
        <result column="id_showtime" property="id_showtime"/>
    </resultMap>

</mapper>